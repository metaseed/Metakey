      <Private>false</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\lib\Metaseed.MetaPlugin\Metaseed.MetaPlugin.csproj">
      <Private>false</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\lib\Metaseed.MouseKeyHook\Metaseed.MouseKeyHook.csproj">
      <Private>false</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\lib\Metaseed.UI\Metaseed.UI.csproj">
      <Private>false</Private>
    </ProjectReference>
    <ProjectReference Include="..\..\lib\Metaseed.WindowsInput\Metaseed.WindowsInput.csproj">
      <Private>false</Private>
    </ProjectReference>
  </ItemGroup>

